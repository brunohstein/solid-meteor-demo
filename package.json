{
  "name": "meteor-solidjs-test",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "build": "tsc",
    "test": "tsc --noEmit",
    "test-mocha-notyet": "meteor test --once --driver-package meteortesting:mocha",
    "test-app-notyet": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "visualize": "meteor --production --extra-packages bundle-visualizer"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "meteor-node-stubs": "^1.1.0",
    "solid-js": "^1.2.6"
  },
  "devDependencies": {
    "@types/meteor": "^1.4.86",
    "@types/node": "^17.0.5",
    "babel-preset-solid": "^1.2.6",
    "typescript": "^4.5.4",
    "typings": "^2.1.1"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.tsx",
      "server": "server/main.ts"
    },
    "testModule": "tests/main.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "solid"
    ]
  }
}
